{
    "name": "douglas/cli-file-reader",
    "description": "A simple PHP CLI tool to read `.csv` files and import data into a MySQL database using Docker Compose.",
    "type": "project",
    "require": {
        "php": "~8.3",
        "robmorgan/phinx": "^0.16.10",
        "ext-pdo": "*",
        "symfony/console": "^7.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/Helpers/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Douglas Vicentini",
            "email": "douglas.dvferreira@gmail.com"
        }
    ],
    "require-dev": {
        "phpunit/phpunit": "^12.3"
    }
}
